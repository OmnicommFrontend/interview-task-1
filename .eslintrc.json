{
    "env": {
        "browser": true,
        "es2021": true
    },
    "ignorePatterns": [
        "**.config.**"
      ],
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "overrides": [
        {
        "files": [
            "*.js"
          ]
    }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
